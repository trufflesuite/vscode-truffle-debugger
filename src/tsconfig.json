{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",

    "noImplicitAny": false,
    "removeComments": false,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "inlineSourceMap": false,
    "sourceMap": true,
    "outDir": "../out/src",
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "noUnusedParameters": false
  }
}